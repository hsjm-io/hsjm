  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsjm-io/hsjm.git",
    "directory": "/home/shorwood/workspaces/hsjm/packages/unocss-preset"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
