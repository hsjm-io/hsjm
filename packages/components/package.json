  "dependencies": {
    "@hsjm/core": "workspace:*",
    "@hsjm/iconify": "workspace:*",
    "@hsjm/leaflet": "workspace:*",
    "@hsjm/mapbox": "workspace:*",
    "@vueuse/core": "*",
    "@vueuse/shared": "*",
    "vue-demi": "*"
  }
}
