{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 14",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "types": [
      "@types/node",
      "@types/lodash"
    ],
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["./src/*"]
}
