{
  "name": "pompaute",
  "version": "1.0.7",
  "description": "Collection of common components, composables and utils for Vue projects.",
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./src/*",
    "./components/*": "./src/components/*",
    "./composables/*": "./src/composables/*",
    "./utils/*": "./src/utils/*"
  },
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config -w"
  },
  "dependencies": {
    "@iconify/iconify": "^2.1.2",
    "@iconify/json": "^2.1.8",
    "@vueuse/components": "^7.7.0",
    "@vueuse/core": "^7.7.0",
    "@vueuse/shared": "^7.7.0",
    "axios": "^0.26.0",
    "lodash": "^4.17.21",
    "milsymbol": "^2.0.0",
    "tinycolor2": "^1.4.2",
    "vue-demi": "^0.12.1",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@optimize-lodash/rollup-plugin": "^3.0.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/color": "^3.0.3",
    "@types/lodash": "^4.14.179",
    "@types/node": "^16.11.26",
    "@types/tinycolor2": "^1.4.3",
    "firebase": "^9.6.7",
    "rollup": "^2.68.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.3.1",
    "typescript": "4.5.5",
    "unplugin-vue-components": "^0.17.21",
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "keywords": [
    "utils",
    "vue",
    "vue-use",
    "vue-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shorwood/pompaute.git"
  },
  "license": "MIT"
}
