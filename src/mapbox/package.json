{
  "name": "@hsjm/mapbox",
  "version": "1.0.0",
  "description": "Mapbox composables and utils.",
  "license": "MIT",
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./index.cjs",
  "jsdelivr": "./index.iife.min.js",
  "unpkg": "./index.iife.min.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "dependencies": {
    "@vueuse/core": "*",
    "@vueuse/shared": "*",
    "vue-demi": "*"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.1.0",
    "vue": "^2.6.0 || ^3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "globals": {
    "@mapbox/mapbox-sdk": "mapbox-sdk"
  }
